<?xml version="1.0" encoding="UTF-8" ?>
<dds>
  <participant profile_name="default">
    <rtps>
      <!-- Disable default transports -->
      <useBuiltinTransports>false</useBuiltinTransports>

      <!-- Define UDP transport configuration -->
      <userTransports>
        <transport_id>udp</transport_id>
      </userTransports>

      <transport>
        <transport_id>udp</transport_id>
        <type>UDPv4</type>
        <multicast_enabled>false</multicast_enabled>
      </transport>

    </rtps>
  </participant>
</dds>
